{
  "fileMatch": "package.json",
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "schedule": [
    "after 10pm and before 5am every weekday",
    "every weekend"
  ],
  "commitMessagePrefix": "chore(deps):"
}
